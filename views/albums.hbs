<div class="container">
    <div class="row">
        {{#each album}}
        <div class="col-lg-4 album-card">
            <img src="{{this.images.0.url}}" alt="Group/Artist" width="100%">
            <div class="card-container">
                <p>{{this.name}}</p>
                <a href="/tracks/{{this.id}}">View Tracks</a>
            </div>
        </div>
        {{/each}}
    </div>
</div>